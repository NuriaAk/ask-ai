{
  "name": "ask-ai",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n backend,frontend -c green,blue \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "PYTHONPATH=backend python -m uvicorn app.main:app --host 0.0.0.0 --port 3000",
    "dev:frontend": "npm --prefix frontend run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
